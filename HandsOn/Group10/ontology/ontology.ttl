 @prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix monu: <http://MonumentMadrid.com> .
@prefix ont: <http://MonumentMadrid.com/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

monu: a owl:Ontology ;
    dc:creator "Javier Rios Serrano" ;
    dc:description "Ontology code created by Chowlk" ;
    dc:title "Monuments Ontology" ;
    owl:versionInfo "0.0.1" .

ont:addressName a owl:DatatypeProperty ;
    rdfs:label "address name" ;
    rdfs:range xsd:string .

ont:addressType a owl:DatatypeProperty ;
    rdfs:label "address type" ;
    rdfs:range xsd:string .

ont:city a owl:DatatypeProperty ;
    rdfs:label "city" ;
    rdfs:range xsd:string .

ont:contentURL a owl:DatatypeProperty ;
    rdfs:label "content u r l" ;
    rdfs:range xsd:uRI .

ont:coordX a owl:DatatypeProperty ;
    rdfs:label "coord x" ;
    rdfs:range xsd:string .

ont:coordY a owl:DatatypeProperty ;
    rdfs:label "coord y" ;
    rdfs:range xsd:string .

ont:date a owl:DatatypeProperty ;
    rdfs:label "date" ;
    rdfs:range xsd:integer .

ont:description a owl:DatatypeProperty ;
    rdfs:label "description" ;
    rdfs:range xsd:string .

ont:designedBy a owl:ObjectProperty ;
    rdfs:label "designed by" .

ont:district a owl:DatatypeProperty ;
    rdfs:label "district" ;
    rdfs:range xsd:string .

ont:districtCode a owl:DatatypeProperty ;
    rdfs:label "district code" ;
    rdfs:range xsd:integer .

ont:hasLocation a owl:ObjectProperty ;
    rdfs:label "has location" .

ont:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:range xsd:string .

