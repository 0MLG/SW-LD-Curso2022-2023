    rdfs:range xsd:string .

ont:neighborhoodCode a owl:DatatypeProperty ;
    rdfs:label "neighborhood code" ;
    rdfs:range xsd:integer .

ont:number a owl:DatatypeProperty ;
    rdfs:label "number" ;
    rdfs:range xsd:integer .

ont:pK a owl:DatatypeProperty ;
    rdfs:label "p k" ;
    rdfs:domain monu:Monument ;
    rdfs:range xsd:integer .

ont:postalCode a owl:DatatypeProperty ;
    rdfs:label "postal code" ;
    rdfs:range xsd:integer .

ont:province a owl:DatatypeProperty ;
    rdfs:label "province" ;
    rdfs:range xsd:integer .

ont:subtype a owl:DatatypeProperty ;
    rdfs:label "subtype" ;
    rdfs:range xsd:string .

monu:Authors a owl:Class ;
    rdfs:label "Authors" ;
    rdfs:subClassOf ont:Authors .

monu:Localization a owl:Class ;
    rdfs:label "Localization" ;
    rdfs:subClassOf ont:Localization .

ont:Authors a owl:Class ;
    rdfs:label "Authors" .

ont:Localization a owl:Class ;
    rdfs:label "Localization" .

ont:Monument a owl:Class ;
    rdfs:label "Monument" .

monu:Monument a owl:Class ;
    rdfs:label "Monument" ;
    rdfs:subClassOf ont:Monument .

